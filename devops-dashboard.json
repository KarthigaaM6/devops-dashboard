{
   "kind":"devops-dashboard-eks",
   "apiVersion":"v1",
   "metadata":{
      "name":"devops",
      "labels":{
         "app":"devops",
         "role":"master"
      }
   },
   "spec":{
      "replicas":1,
      "selector":{
         "app":"devops",
         "role":"master"
      },
      "template":{
         "metadata":{
            "labels":{
               "app":"devops",
               "role":"master"
            }
         },
         "spec":{
            "containers":[
               {
                  "name":"devops",
                  "image":"191758827001.dkr.ecr.us-west-2.amazonaws.com/devops-dashboard-eks:latest",
                  "ports":[
                     {
                        "name":"devops",
                        "containerPort":3000
                     }
                  ]
               }
            ]
         }
      }
   }
}
